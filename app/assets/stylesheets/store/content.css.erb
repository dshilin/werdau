.compare {
  border-top: 1px dashed #eeeef0;
  border-bottom: 1px dashed #eeeef0;
  font-size: 12px;
  padding: 10px 10px 10px 36px;
}

.compare .del {
  display: inline-block;
  width: 12px;
  height: 12px;
  background: url(<%= image_path 'store/del.png' %>) no-repeat;
  line-height: 0;
  margin-bottom: -3px;
}

.additionalnav a {
  border-bottom: 1px dashed #d82123;
  font-size: 19px;
  text-decoration: none;
  display: inline-block;
  margin-bottom: 6px;
  line-height: 18px;
}

.additionalnav a:hover {
  color: #f8dbd3;
}

.additionalnav .callcenter {
  width: 186px;
  height: 68px;
  display: block;
  background: url(<%= image_path 'store/callme.png' %>) no-repeat;
  position: absolute;
  bottom: -12px;
  left: -10px;
  border-width: 0px;
}

.additionalnav  .callcenter:hover {
  background: url(<%= image_path 'store/callmeon.png' %>) no-repeat;  
  padding: 0px;
}

.callcenter span {
  display: none;
}

.nav a {
  display: inline-block;
  margin: 0 5px;
  text-decoration: none;
  color: #565656;  
}      

.nav a:hover {
  text-decoration: underline;
}

.nav a.last {
  width: 13px;
  height: 13px;
  background: url(<%= image_path 'store/arrowr.png' %>) no-repeat;  
  position: relative;
  bottom: -1px;
  margin: 0;
}

.nav a.next {
  width: 13px;
  height: 13px;
  background: url(<%= image_path 'store/arrowl.png' %>) no-repeat;  
  position: relative;
  bottom: -1px;
  margin: 0;
}

.index .col-2 h2 {
  text-transform: uppercase;
  margin: 20px 0 15px 0;
  color: #4a6d80;
}

.index .col-2 a {
  color: #babcc8;
}

.index .col-2 p.date {
  margin-bottom: 0;
}

.index .col-2 p.date span{
  background: #a2adb1;
  color: #fff;
}

.index .col-2 p.pre {
  margin-bottom: 15px;
}

.index .col-2 p.all {
  margin-bottom: 15px;
  margin-top: -10px;
  text-align: right;
}

.index .col-2 p.all a {
  color: #4c6d80;

}

.index .col-2 .news .promo h2 {
  margin: -2px 0 15px 0;  
  color: #d8201e;
  font-size: 20px;
}

.index .col-2 .news .promo .all a {
  color: #d8201e;
}

.index .col-2 .news .promo p.date span {
  background: #d8201e;
}

.about .textpage {
  background: #efefef;
  border-right: 5px solid #fff;
  border-top: 5px solid #fff;
  padding: 25px 30px 25px 40px;  
  font-size: 13px;
}

.about .corner {
  background: url(<%= image_path 'store/corner.png' %>) no-repeat right top; 
  position: absolute;
  z-index: 100;
  width: 50px;
  height: 70px;
  top: -15px;
  right: -5px;
}

.about .textpage h3 {
  font-weight: normal;
  font-size: 18px;
  color: #d8201e;
  margin: 0 0 20px 0;
}

.about .textpage a {
  color: #d8201e;
}

.more {
  text-align: right;  
}

.more {
  background: url(<%= image_path 'store/more.png' %>) no-repeat right;   
  padding-right: 15px;
}

.index .col-1 h1 {
  font-size: 30px;
  color: #b9bdc8;
  text-transform: uppercase;
  font-weight: normal;
  margin-bottom: 60px;
}

.index .recommendations {
  background: url(<%= image_path 'store/rec.png' %>) no-repeat ; 
  width: auto;
  overflow: hidden;
}

.index .good {
  float: left;
  width: 214px;
  height: 250px;
  padding:  0 0 0 20px;
}

.index .good h2 {
  color: #226184;
  font-weight: normal;
}

.index .good a {
  color: #1d1d1b;
  font-size: 18px;
  text-decoration: none;
  line-height: 13px;
}

.index .good h2 a:hover {
  text-decoration: underline;
}

.index .good .price {
  display: block;
  font-size: 18px;
  padding-left: 15px;
  background: url(<%= image_path 'store/line2.png' %>) no-repeat left; 
}

.index .good a .name {
  color: #717171;
  font-size: 13px;
  text-decoration: underline;
  padding-right: 15px;
  line-height: 12px;
}

.index .good img {
  display: block;
  margin: 10px auto;  
  margin-bottom: 20px;
}

.index .col-3 h2 {
  float: right;
  background: url(<%= image_path 'store/header.png' %>) repeat-x top; 
  text-transform: uppercase;
  font-weight: normal;
  height: 22px;
  line-height: 22px;
  position: relative;
  margin: 0 5px 20px 0;
  padding: 0 5px;
  font-size: 13px;
}

.index .col-3 h2 .lc{
  position: absolute;
  top: 0;
  left: -4px;
  display: block;
  width: 4px;
  height: 22px;
  background: url(<%= image_path 'store/lc2.png' %>) no-repeat;
}

.index .col-3 h2 .rc{
  position: absolute;
  top: 0;
  right: -4px;
  display: block;
  width: 4px;
  height: 22px;
  background: url(<%= image_path 'store/rc2.png' %>) no-repeat;
}

.index .schedule {
  clear: right;
  width: auto;
  overflow: hidden;
  margin-bottom: 20px;
}

.index .schedule .tab {
  position: relative;
  width: auto;
  overflow: hidden;
  height: 30px;
  font-size: 13px;
  letter-spacing: -0.1em;
  margin-bottom: 10px;
}

.index .schedule .tab span {
  float: left;
}

.index .schedule .tab a {
  display: block;
  margin-bottom: 2px;
}

.index .schedule .tab div {
  float: left;
  padding-left: 20px;
  background: url(<%= image_path 'store/line3.png' %>) no-repeat left 0px;
  position: absolute;
  left: 125px;
  height: 28px;   
  white-space: nowrap;
  line-height: 12px;
}

.index .schedule .day {
  float: left;
  font-weight:bold;
  width: 60px;
  font-size: 15px;
  text-align: right;
  line-height: 25px;
  white-space: no-wrap;
}

.index .schedule .time {
  float: left;
  font-size: 25px;
  line-height: 25px;
  font-weight: bold;
  padding-left: 7px;
  color: #d82022;
  margin: -3px 0 0 0;
  letter-spacing: -0.1em;
  white-space: no-wrap;
  width: 80px;
}

.justwatched {
  clear: right;
  max-height: 200px;
  overflow: auto;
  margin-bottom: 30px;
}

.justwatched img {
  float: left;
}

.justwatched li {
  margin-bottom: 10px;
}

.justwatched a {
  color: #545a5a;
}

.justwatched .name, .justwatched .price {
  display: block;
  margin-left: 60px;
}

.justwatched .name {
  font-weight: bold;
}

.justwatched .price {
  text-decoration: none;
}

.auction {
  text-align: center;
  position: relative;
}

.auction img {
  float: left;
}

.auction a {
  color: #717e87;
  text-decoration: none;
}

.auction a .name, .auction a .price {
  display: block;
  text-align: left;
  margin-left: 70px;
}

.auction a .name {
  text-decoration: underline;
}

.auction a .price {
  font-size: 35px;
  color: #489fba;
  font-weight: bold;
  white-space: nowrap;
}

.clearright {
  clear: right;
}

.bpanel .tab {
  display: block;
  float: left;
  position: relative;
  background: url(<%= image_path 'store/tab.png' %>) repeat-x top;
  height: 26px;
  padding: 0 5px;
  margin: 0 15px;
  color: #fff;
  text-decoration: none;
  line-height: 21px;
}

.bpanel .tab:hover {
  text-decoration: underline;
  top: 1px;
}

.bpanel .tab .rc {
  position: absolute;
  top: 0;
  right: -9px;
  display: block;
  width: 9px;
  height: 26px;
  background: url(<%= image_path 'store/rc.png' %>) no-repeat;
}

.bpanel .tab .lc {
  position: absolute;
  top: 0;
  left: -8px;
  display: block;
  width: 8px;
  height: 26px;
  background: url(<%= image_path 'store/lc.png' %>) no-repeat;
}

.bpanel .l1 .ic {
  background: url(<%= image_path 'store/ic1.png' %>) no-repeat left top;
  padding-left: 18px;
}

.bpanel .l2 .ic {
  background: url(<%= image_path 'store/ic2.png' %>) no-repeat left -1px;
  padding-left: 18px;
}

.bpanel .l3 .ic {
  background: url(<%= image_path 'store/ic3.png' %>) no-repeat left -1px;
  padding-left: 18px;
}

.bpanel .l4 .ic {
  background: url(<%= image_path 'store/ic4.png' %>) no-repeat left -1px;
  padding-left: 18px;
}

.bpanel .l5 .ic {
  background: url(<%= image_path 'store/ic5.png' %>) no-repeat left -1px;
  padding-left: 22px;
}

.bpanel .l6 {
  width: 170px;
  height: 42px;
  background: url(<%= image_path 'store/ic7.png' %>) no-repeat top left;
  display: block;
  float: left;
  padding: 7px 5px 0 30px;
  margin: 0 15px;
  color: #fff;
  text-decoration: none;
  position: relative;
  top: -15px;
  line-height: 11px;
}

.bpanel .l6 span {
  display: block;
}

.bpanel .l6 span.call {
  text-transform: uppercase;
  font-size: 11px;
}

.bpanel .l6:hover {
  text-decoration: underline;
  top: -14px;
}
